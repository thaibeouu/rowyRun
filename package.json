{
  "name": "rowy-run",
  "description": "cloud run instance for running Rowy's backend functionality",
  "version": "1.0.0",
  "private": true,
  "main": "build/index.js",
  "scripts": {
    "start": "node build/index.js",
    "dev": "nodemon",
    "test": "mocha test/index.test.js --exit",
    "system-test": "NAME=Cloud mocha test/system.test.js --timeout=180000",
    "lint": "eslint '**/*.js'",
    "fix": "eslint --fix '**/*.js'",
    "build": "tsc;cpy --cwd=src --parents '**/*.json' ../build/"
  },
  "engines": {
    "node": "14"
  },
  "author": "ROWY LLC",
  "license": "Apache-2.0",
  "dependencies": {
    "@google-cloud/bigquery": "^5.8.0",
    "@google-cloud/firestore": "^4.15.0",
    "@google-cloud/secret-manager": "^3.10.0",
    "algoliasearch": "^4.10.5",
    "axios": "^0.21.4",
    "cpy-cli": "^2.0.0",
    "express": "^4.17.1",
    "firebase-admin": "^9.11.1",
    "firebase-functions": "^3.15.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.6.1",
    "firebase-tools": "^9.18.0",
    "google-auth-library": "^7.0.0",
    "husky": "^4.3.8",
    "js-beautify": "^1.13.0",
    "mocha": "^9.0.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.0",
    "pretty-quick": "^3.1.1",
    "supertest": "^6.0.0",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.0",
    "typescript": "^4.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
